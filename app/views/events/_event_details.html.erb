<div class="container-game-details" id="game-details">
  <h3> <%= event.sport.name %> </h3>
        <div> <strong><%= event.title %></strong> </div>
        <div>creator : <%= event.user.name %></div>
        <div> <%= event.description %> </div>
        <div>  <i class="fa-solid fa-clock"></i> <%= event.date %> </div>
        <div>
          <i class="fa-solid fa-location-dot"></i>
          <%= event.location.name %>,
          <%= event.location.address %>,
          <%= event.location.city %>
        </div>
        <div> <%= event.bookings.count %>/<%= event.capacity %> players </div>
        <div id="game-data" data-booking-id= <%= @booking_id %> data-event-id="<%= @event.id %>" data-player-id="<%= current_user.id %>">
        </div>
</div>
