<section>
  <div class="section about-section gray-bg" id="about">
    <div class="container">
      <div class="row align-items-center flex-row-reverse">
        <div class="col-md-6">
            <div class="about-text go-to">
                <h3 class="dark-color"><%= @user.name %></h3>
                <h6 class="theme-color lead">Bio</h6>
                <p><%= @user.bio %></p>
                <div class="row about-list">
                    <div class="col-md-6">
                        <div class="media">
                            <label>Age</label>
                            <p><%= @user.age %></p>
                        </div>
                        <div class="media">
                            <label>Rating</label>
                            <p><%= @user.rating %> Stars</p>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="media">
                            <label>E-mail</label>
                            <p><%= @user.email %></p>
                        </div>
                        <div class="media">
                            <label>Telephone</label>
                            <p><%= @user.phone_number %></p>
                        </div>
                       <br>
                        <a href="#">Editer le profil</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
          <div class="about-avatar">
              <img src="https://bootdey.com/img/Content/avatar/avatar7.png" title="" alt="">
          </div>
        </div>
      </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-md-5">
      <h3>Vos annonces</h3>
      <h3>Vos participations</h3>
      <h3>En attente</h3>
      </div>
       <div class="col-md-7">
        <div class="container">
          <div class="row">
            <div class="col-md-4" style="background: grey">
            <p>item 1</p>
            </div>
            <div class="col-md-4" style="background: grey">
            <p>item 2</p>
            </div>
            <div class="col-md-4" style="background: grey">
            <p>item 3</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
